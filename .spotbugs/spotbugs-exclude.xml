    <Match>
        <!-- we don't deal with non ascii strings for toUpperCase -->
        <Bug pattern="DM_CONVERT_CASE" />
    </Match>
    <Match>
        <!-- we know configure is called before serialize -->
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" />
