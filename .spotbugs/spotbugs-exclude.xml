    </Match>
    <Match>
        <!-- we know configure is called before deserialize -->
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" />
        <Class name="se.irori.kafka.claimcheck.ClaimCheckDeserializer" />
