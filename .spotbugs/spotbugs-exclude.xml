    <Match>
        <!-- we know configure is called before onSend -->
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" />
        <Class name="se.irori.kafka.claimcheck.ClaimCheckProducerInterceptor" />
    </Match>
    <Match>
        <!-- we know configure is called before serialize -->
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" />
        <Class name="~se\.irori\.kafka\.claimcheck\.azure\.AzureBlobStorageClaimCheckBackend" />
    </Match>
    <Match>
        <!-- we know configure is called before serialize -->
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR" />
